.wrap{
  display:flex;
  flex-direction:column;
  border-top:1px solid #efefef;
  border-bottom:1px solid #efefef;
  padding:5px 0;
  margin:20rpx 0 40rpx;
}
.avatar{
  margin: 10rpx 0 20rpx 10rpx;
}
.avatar image{
  width:60rpx;
  height:60rpx;
  vertical-align:middle;
  margin-right: 10rpx;
}
.jiaoyu{
  font-size: 18px;
  font-weight: bold;
  margin:10rpx;
}
.carouselImg{
  width:100%;
  height:340rpx;
}
.textDetail{
  font-size:16px;
  color:#666;
  text-indent:16px;
  line-height:28px;
}
.viewImg{
  height: 60rpx;
}
.viewImg image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.viewImg text{
  font-size: 14px;
  margin-right: 10rpx;
}